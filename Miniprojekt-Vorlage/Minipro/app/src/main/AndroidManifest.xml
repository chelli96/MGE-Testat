<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.ben_d.minipro">
    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity
            android:name="com.example.ben_d.minipro.LoginActivity">
        <intent-filter>
            <action android:name="android.intent.action.MAIN" />

            <category android:name="android.intent.category.LAUNCHER" />
        </intent-filter>
        </activity>

        <activity
        android:name="com.example.ben_d.minipro.RegistrationActivity" android:parentActivityName=".LoginActivity">
    </activity>

        <activity
            android:name="com.example.ben_d.minipro.HomeActivity">
        </activity>

        <activity
            android:name="com.example.ben_d.minipro.LoanActivity" android:parentActivityName=".HomeActivity" android:label="loans">
        </activity>

        <activity
            android:name="com.example.ben_d.minipro.ReservationActivity" android:parentActivityName=".HomeActivity" android:label="reservations">
        </activity>

    </application>

</manifest>